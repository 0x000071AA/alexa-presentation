{
  "extends": "airbnb",
  "rules": {
    "max-len": [
      "error",
      120
    ],
    "consistent-return": "off",
    "comma-dangle": 0,
    "space-before-function-paren": 0,
    "class-methods-use-this": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-console": 0,
    "no-continue": 0,
    "no-restricted-syntax": 0,
    "newline-per-chained-call": 0,
    "object-shorthand": 0,
    "camelcase": 0,
    "global-require": 0,
    "import/prefer-default-export": 0,
    "no-unused-vars": [
      2,
      {
        "vars": "all",
        "args": "none"
      }
    ]
  }
}
